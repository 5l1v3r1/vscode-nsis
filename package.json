            },
            {
                "language": "nsis",
                "path": "./snippets/header.WinVer.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/header.WordFunc.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/header.x64.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/plugin.AdvSplash.json"
            },
            {
