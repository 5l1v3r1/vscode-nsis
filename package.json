{
    "name": "nsis",
    "displayName": "NSIS",
    "description": "Language syntax and snippets for Nullsoft Scriptable Install System (NSIS)",
    "version": "0.4.0",
    "publisher": "idleberg",
    "license": "MIT OR Apache-2.0",
    "author": {
        "name": "Jan T. Sott",
        "url": "http://github.com/idleberg"
    },
    "keywords": [
        "nsis",
        "nullsoft",
        "installer",
        "setup",
        "windows"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/idleberg/vscode-nsis.git"
    },
    "bugs": {
        "url": "https://github.com/idleberg/vscode-nsis/issues"
    },
    "icon": "images/logo.svg",
    "engines": {
        "vscode": ">=1.0.0 <2.0.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "nsis",
                "aliases": [
                    "NSIS",
                    "nsis"
                ],
                "extensions": [
                    ".nsi",
                    ".nsh"
                ],
                "configuration": "./nsis.configuration.json"
            },
            {
                "id": "nlf",
                "aliases": [
                    "NSIS Language File",
                    "nlf"
                ],
                "extensions": [
                    ".nlf"
                ],
                "configuration": "./nlf.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nsis",
                "scopeName": "source.nsis",
                "path": "./syntaxes/nsis.tmLanguage"
            },
            {
                "language": "nlf",
                "scopeName": "source.nlf",
                "path": "./syntaxes/nlf.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "ini",
                "path": "./snippets/core.InstallOptions.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/core.DrunkenNSIS.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/core.InstallOptions.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/core.NSIS.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/core.Options.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/core.Predefines.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/core.Variables.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/header.FileFunc.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/header.LogicLib.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/header.Memento.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/header.ModernUI.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/header.MultiUser.json"
            },
            {
                "language": "nsis",
                "path": "./snippets/header.Sections.json"
            },
            {
